{
  "name": "thewunder/corma-bundle",
  "description": "Symfony Bundle for the Corma ORM",
  "type": "library",
  "keywords": ["Object Relational Mapper", "Data Mapper", "ORM", "Database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Michael O'Connell",
      "email": "wundbread@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": { "Corma\\OrmBundle\\" : "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Corma\\OrmBundle\\Test\\": "tests/"}
  },
  "require": {
    "php":">=8.1",
    "thewunder/corma": "^5.0",
    "symfony/config": "^6.0|^7.0",
    "symfony/http-kernel": "^6.0|^7.0",
    "symfony/dependency-injection": "^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  }
}
